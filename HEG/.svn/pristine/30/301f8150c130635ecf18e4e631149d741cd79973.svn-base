/*html,body {font-size: 23.4375px!important;}*/
html,body{
	width:100%;
	height:100%;
	display:block;
	color:#666;
	box-sizing: border-box;
}
a{
	text-decoration:none;
	cursor:pointer;
}
.wrapper{
	width:100%;
	height:100%;
}
.container{
	height:100%;
	background-color:#efeff4;
	-webkit-overflow-scrolling:touch;
	overflow:auto;
}
input[type='text'],input[type='password']{
	font-size:0.768rem;
	color:#666;
	text-indent:1rem;
}
input,button,textarea{
	border:none;
	outline:none;
	-webkit-appearance: none;
    appearance: none;
}
select{
	-webkit-appearance: none;
    appearance: none;
    outline: none;
    background-color:transparent;
}
.clear{
	clear:both;
	display:table;
	content:'';
}
.clearm{
	display:table;
	content:'';
}
.left{
	float:left;
}
.right{
	float:right;
}
span{
	display:inline-block;
}
.flex{
	display:-webkit-box;
	display:-moz-box;
	display: box;
	display:-ms-flexbox;
	display:-webkit-flex;
	display:flex;
}
.space-between{
	box-pack:justify;
	-webkit-box-pack: justify;
	 -moz-justify-content: space-between;
	 -webkit-justify-content: space-between;
	 justify-content: space-between;
}
.content-center{
	 box-pack:center;
	 -webkit-box-pack: center;
	 -moz-justify-content: center;
	 -webkit-justify-content: center;
	 justify-content: center;
}
.content-start{
	 box-pack:start;
	 -webkit-box-pack: start;
	 -moz-justify-content: flex-start;
	 -webkit-justify-content: flex-start;
	 justify-content: flex-start;
}
.content-end{
	box-pack:end;
	-webkit-box-pack: end;
	 -moz-justify-content: flex-end;
	 -webkit-justify-content: flex-end;
	 justify-content: flex-end;
}
.align-items-center{
	-webkit-box-align: center;
	 -moz-align-items: center;
	 -webkit-align-items: center;
	 align-items: center;
}
.direction-column{
	-webkit-box-direction: normal;
	 -webkit-box-orient: vertical;
	 -moz-flex-direction: column;
	 -webkit-flex-direction: column;
	 flex-direction: column;
}

.hide{
	display:none;
}
i{
	font-style:normal;
}
.upcoming{
	background-color:#b1dfd4!important;
	color:#0b9d78!important;
}
.upcomings{
	color:#d4efe8!important;
}
.notpay{
	background-color:#ffeac5!important;
	color:#fdbe4a!important;
}
.notpays{
	color:#fdbe4a!important;
}
.other{
	background-color:#efefef;
	color:#ccc;
}
.others{
	color:#ccc;
}
.yellow{
		color:#ffad3d!important;
}
.green{
	color:#0b9d78;
}
.blue{
	color:#60c9f8;
}

::selection{ background:none;}
::-webkit-selection { background:none;}
::-webkit-scrollbar{ display:none;}

 .mark{
    position:fixed;
    width:100%;
    height:100%;
    background-color:rgba(0,0,0,.5);
    z-index:18;
}
.animated {
  -webkit-animation-duration: 0.5s;
  animation-duration: 0.5s;
  animation-fill-mode: both;
  webkit-animation-play-state: both;
}